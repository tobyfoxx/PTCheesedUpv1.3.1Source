function check_boss(obj_index)
{
	return !(obj_index != obj_pepperman && obj_index != obj_noiseboss && obj_index != obj_vigilanteboss
	&& obj_index != obj_fakepepboss && obj_index != obj_pf_fakepep && obj_index != obj_pizzafaceboss
	&& obj_index != obj_pizzafaceboss_p3 && obj_index != obj_pizzafaceboss_p2);
}
